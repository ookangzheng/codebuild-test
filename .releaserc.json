{
  "plugins": [
    [
      "semantic-release-gitmoji",
      {
        "releaseRules": {
          "patch": {
            "include": [
              ":bento:"
            ]
          }
        }
      }
    ],
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "CHANGELOG.md"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "ðŸ”– release: ${nextRelease.version} \n ${nextRelease.notes}"
      }
    ],
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator"
  ],
  "branches": [
    {
      "name": "1.1.x",
      "range": "1.1.x",
      "channel": "1.1.x"
    },
    {
      "name": "1.2.x",
      "range": "1.2.x",
      "channel": "1.2.x"
    },
    {
      "name": "master"
    }
  ]
}
