{
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "master",
    "test",
    {
      "name": "beta",
      "prerelease": "true"
    },
    {
      "name": "alpha",
      "prerelease": "true"
    }
  ],
  "plugins": [
    [
      "semantic-release-gitmoji",
      {
        "releaseRules": {
          "patch": {
            "include": [
              ":bento:"
            ]
          }
        }
      }
    ],
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "CHANGELOG.md"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "ðŸ”– release: ${nextRelease.version} \n ${nextRelease.notes}"
      }
    ],
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator"
  ]
}
